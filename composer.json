{
    "name": "maer/google-auth",
    "description": "Laravel 4 wrapper for the league/oauth2-client package. Simple white/blacklist specific e-mails or entire domains.",
    "keywords": ["google","oauth2", "laravel"],
    "authors": [
        {
            "name": "Magnus Eriksson",
            "email": "maer@reloop.se"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.1.*"
    },
    "require-dev": {
        "league/oauth2-client": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "Maer\\GoogleAuth\\": "src/"
        }
    },
    "minimum-stability": "dev"
}
